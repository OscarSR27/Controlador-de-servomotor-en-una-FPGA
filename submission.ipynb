        "# Code for Exercise 1\n",
        "\n",
        "def factorial(n):\n",
        "  if n == 0:\n",
        "    result = 1\n",
        "  else:\n",
        "    result = n * factorial(n-1)\n",
        "\n",
        "  return result\n"
      ]
